[core]
	autocrlf = false
	safecrlf = true
	excludesfile = ~/.gitignore
[alias]
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  	type = cat-file -t
  	dump = cat-file -p

        aa = add -A
        cb = checkout -b
        ci = commit
        co = checkout
        cm = checkout master
        st = status
        dc = diff --cached
[color]
	ui = true
[push]
	default = simple
